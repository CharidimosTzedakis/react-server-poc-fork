{
  "name": "rsc-micro-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:host": "react-server ./src/index.tsx --name host --port 3000",
    "dev:remote": "react-server ./src/remote.tsx --name remote --host 127.0.0.1 --port 3001",
    "dev:counter": "react-server ./src/counter.tsx --name counter --host 127.0.0.1 --port 3002",
    "dev:form": "react-server ./src/form.tsx --name form --host 127.0.0.1 --port 3003 --cors",
    "dev:streaming": "react-server ./src/streaming.tsx --host 127.0.0.1 --port 3004 --name streaming",
    "dev": "run-p dev:host dev:remote dev:counter dev:form dev:streaming",
    "build:host": "react-server build ./src/index.tsx --outDir .react-server --no-export",
    "build:remote": "react-server build ./src/remote.tsx --outDir .react-server-remote",
    "build:counter": "react-server build ./src/counter.tsx --outDir .react-server-counter --no-export",
    "build:form": "react-server build ./src/form.tsx --outDir .react-server-form --no-export",
    "build:streaming": "react-server build ./src/streaming.jsx --outDir .react-server-streaming --no-export",
    "build": "run-s build:host build:remote build:counter build:form",
    "start:host": "react-server start --outDir .react-server --port 3000",
    "start:remote": "react-server start --outDir .react-server-remote --host 127.0.0.1 --port 3001",
    "start:counter": "react-server start --outDir .react-server-counter --host 127.0.0.1 --port 3002 --cors",
    "start:form": "react-server start --outDir .react-server-form --host 127.0.0.1 --port 3003 --cors",
    "start:streaming": "react-server start --outDir .react-server-streaming --host 127.0.0.1 --port 3004 --cors",
    "start": "run-p start:host start:remote start:counter start:form start:streaming"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lazarv/react-server": "workspace:^"
  },
  "devDependencies": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
